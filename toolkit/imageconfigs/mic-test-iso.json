{
    "SystemConfigs": [
        {
            "Name": "baremetal-iso",
            "Hostname": "baremetal-iso-host-name",
            "KernelOptions": {
                "default": "kernel",
                "hyperv": "kernel"
            },
            "KernelCommandLine": {
                "ExtraCommandLine": "overlay-size=70% selinux=0 console=tty0 apparmor=0",
                "SELinux": "permissive"
            },
            "AdditionalFiles": {
                "files/rootfs.img": "/rootfs.img"
            }
        }
    ]
}
